CompileFlags:
  Add: 
    - -std=c++20
    - -O2
    - -Wall
    - -Wextra
    - -pedantic
    - -Wshadow
    - -Wformat=2
    - -Wfloat-equal
    - -Wconversion
    - -Wlogical-op
    - -Wshift-overflow=2
    - -Wduplicated-cond
    - -Wcast-qual
    - -Wcast-align
    - -D_GLIBCXX_DEBUG
    - -fmax-errors=1
    - -Winvalid-pch
    - -DDEBUG
    - -I"D:/Personal/Scripts"
    - -I"D:/WinLibs/mingw64/include"
    - -I"D:/WinLibs/mingw64/x86_64-w64-mingw32/include"
    - -L"D:/WinLibs/mingw64/lib"
    - --target=x86_64-w64-mingw32
    - -static-libgcc
    - -static-libstdc++
